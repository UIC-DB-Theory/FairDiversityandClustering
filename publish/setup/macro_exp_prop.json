{
    "datasets": {
        "Popsim_1M": {
            "data_dir": "./datasets/popsim_1M",
            "color_fields": [
                "race"
            ],
            "feature_fields": [
                "x",
                "y",
                "z"
            ],
            "normalize": true,
            "filter_unique": false
        },
        "Adult": {
            "data_dir": "./datasets/adult",
            "color_fields": [
                "race",
                "sex"
            ],
            "feature_fields": [
                "age",
                "capital-gain",
                "capital-loss",
                "hours-per-week",
                "fnlwgt",
                "education-num"
            ],
            "normalize": true,
            "filter_unique": false
        },
        "Diabetes": {
            "data_dir": "./datasets/diabetes",
            "color_fields": [
                "gender",
                "diabetesMed"
            ],
            "feature_fields": [
                "num_procedures",
                "num_lab_procedures",
                "number_outpatient",
                "number_inpatient",
                "number_diagnoses",
                "time_in_hospital",
                "num_medications"
            ],
            "normalize": true,
            "filter_unique": false
        },
        "Census": {
            "data_dir": "./datasets/census",
            "color_fields": [
                "age-sex-color"
            ],
            "feature_fields": [
                "attr1",
                "attr2",
                "attr3",
                "attr4",
                "attr5",
                "attr6"
            ],
            "normalize": false,
            "filter_unique": false
        },
        "Popsim": {
            "data_dir": "./datasets/popsim",
            "color_fields": [
                "race"
            ],
            "feature_fields": [
                "x",
                "y",
                "z"
            ],
            "normalize": true,
            "filter_unique": false
        }
    },
    "algorithms": {
        "FMMD-S": {
            "alg": "FMMD-S",
            "color": "#05faee",
            "marker": "s",
            "use_coreset": false,
            "use_dmax": false,
            "use_dmin": false,
            "epsilon": 0.15,
            "timeout": 1800
        },
        "FairFlow": {
            "alg": "FairFlow",
            "color": "#052efa",
            "marker": "p",
            "use_coreset": true,
            "use_dmax": false,
            "use_dmin": false,
            "timeout": 1800
        },
        "FairGreedyFlow": {
            "alg": "FairGreedyFlow",
            "color": "#faac05",
            "marker": "v",
            "use_coreset": true,
            "use_dmax": true,
            "use_dmin": true,
            "epsilon": 0.15,
            "timeout": 1800
        },
        "SFDM-2 (e=.75)": {
            "alg": "SFDM-2",
            "color": "#1b5902",
            "marker": "x",
            "use_coreset": false,
            "use_dmax": true,
            "use_dmin": true,
            "epsilon": 0.75,
            "timeout": 1800
        },
        "SFDM-2 (e=.15)": {
            "alg": "SFDM-2",
            "color": "#4afc03",
            "marker": "+",
            "use_coreset": false,
            "use_dmax": true,
            "use_dmin": true,
            "epsilon": 0.15,
            "timeout": 1800
        },
        "MFD (g=.3)": {
            "alg": "FMMD-MWU",
            "color": "#fc0303",
            "marker": "o",
            "use_coreset": true,
            "use_dmax": true,
            "use_dmin": false,
            "mwu_epsilon": 0.75,
            "falloff_epsilon": 0.15,
            "percent_theoretical_limit": 0.3,
            "timeout": 1800
        }
    },
    "parameters": {
        "k": [
            10,
            101,
            10
        ],
        "observations": 5,
        "buildkis_alpha": 0.1,
        "buildkis_equal_k_js": false
    }
}